{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "store:default",
    "notification:default",
    "dialog:default",
    "shell:default",
    {
      "identifier": "fs:allow-write-text-file",
      "allow": [{ "path": "$HOME/**" }]
    }
  ]
}
